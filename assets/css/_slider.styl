        stroke-width 2px !important

#svg_logo path, #svg_menu path
    stroke-width 1.5px
    stroke #48c1f9

.stroke_white,
#free_demo path
    stroke #aedcf2 !important

#free_account_link
    color #fff
    position relative
    text-decoration none
    font-family 'Roboto', sans-serif
    display inline-block
    text-align center

.hided
    opacity 0

.showed
    opacity 1

.drawsvg
    -webkit-transition opacity 1.25s ease-in-out
    -moz-transition opacity 1.25s ease-in-out
    -ms-transition opacity 1.25s ease-in-out
    -o-transition opacity 1.25s ease-in-out
    transition opacity 1.25s ease-in-out

.drawsvg_line
    position absolute
    max-width 100%
    z-index 10
    top 0
    left 50%
    -webkit-transform translateX(-50%)
    transform translateX(-50%)

.slider_buttons
    display inline-block !important
    padding 12px 55px
    width 260px
    border-radius 25px
    border-color #039be5
    color #4c4c4c
    font-size 15px
    font-weight 500
    z-index 9

.demo-buttons
    color #fff
    position absolute
    bottom 115px
    left 95px

.free_account_text
    background #ffffff
    border 2px solid #ffffff

#free_acct_link,
.demo_account_text.showed
    border 2px solid #fff

.demo_account_text
    background transparent

.slider-separator
    position relative
    margin-top 5px
    .svg_separators
        margin 0px auto 5px auto

#demo_account_text
    .btn-primary
        &.btn-inverted
            border-color #fff

.nav-line-white
    position relative
    width 250px
    margin 10px auto
    .text-or
        font-style italic
        color #fff
        font-weight 300
        &::before
            content ""
            border-top 1px solid #fff
            position absolute
            left 0
            top 10px
            padding 0 50px
