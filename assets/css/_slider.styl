        font-weight 100
        font-size 60px
        color #fff
        margin-top 200px
        margin-bottom 10px
        svg
            position absolute
            top 4px
            margin-left -16px
            path
                stroke-width 2px
    .slider-subheading
        position relative
        font-weight 300
        color #fff
        display inline-block
        margin 0px
        svg
            position absolute
            top 15px
        .sub-heading-text
            padding-top 10px
    .slider-buttons
        margin-top 30px
        #free_demo_link
            background-color transparent
            position relative
            display inline-block
            path
                background-color transparent

#hexas_gsap
    position absolute
    top 10%
    left 40%

#svg_logo, #svg_menu,
#free_account, #free_demo, #phone
